<div>
  <div>
    <h1>お問い合わせ</h1>
    <%= form_for(@contact, url: contacts_new_path) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.text_field :name %>
      <%= f.email_field :email %>
      <%= f.text_area :message %>
      <%= f.submit "送信" %>
    <% end %>
  </div>
</div>
