<div class="center">
  <div>
    <h1>お問い合わせ</h1>
    <%= form_for(@contact, url: contacts_new_path) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.label :name, 'お名前'%>
      <%= f.text_field :name %>
      <%= f.label :email, 'メールアドレス'%>
      <%= f.email_field :email %>
      <%= f.label :message, 'お問い合わせ内容'%>
      <%= f.text_area :message %>
      <%= f.submit "送信" %>
    <% end %>
  </div>
</div>
